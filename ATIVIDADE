public static void main(String[] args) {
int numero = 1;
int multiplicar = 1;        
while (multiplicar <= 10 ){
int valor = numero * mutiplicar;
System.out.println("1 x " + multiplicar + " e igual a: " + valor);
multiplicar = multiplicar + 1;
 }}}
 
